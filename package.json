{
  "name": "netstorage",
  "version": "1.0.0",
  "description": "A modern, ESM-native TypeScript client for the Akamai NetStorage REST API",
  "keywords": [
    "akamai",
    "netstorage",
    "akamai netstorage",
    "netstorageapi",
    "esm",
    "typescript",
    "api",
    "rest",
    "sdk",
    "http"
  ],
  "bugs": {
    "url": "https://github.com/HeavyMedl/netstorage/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HeavyMedl/netstorage.git"
  },
  "license": "UNLICENSED",
  "author": "Kurt Medley <kurtdmedley@gmail.com>",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/src/index.d.ts"
    }
  },
  "module": "./dist/index.js",
  "types": "./dist/src/index.d.ts",
  "bin": {
    "netstorage": "dist/cli.js",
    "nst": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc && tsc-alias",
    "format": "prettier --write .",
    "link:cli": "npm run build && chmod +x dist/cli.js && npm link",
    "lint": "eslint .",
    "lint-staged": "lint-staged",
    "lint:fix": "eslint . --fix",
    "open-coverage": "npm run test && open ./coverage/index.html",
    "prepare": "husky",
    "sandbox:api": "vite-node sandbox/index.ts",
    "sandbox:cli": "vite-node src/cli/index.ts",
    "test": "vitest run --coverage"
  },
  "lint-staged": {
    "package.json": "npx sort-package-json@latest",
    "*.{ts,mts,json,md,yml,mjs}": [
      "npm run format",
      "npm run lint:fix"
    ]
  },
  "dependencies": {
    "commander": "^14.0.0",
    "fast-xml-parser": "^5.2.5",
    "http-status-codes": "^2.3.0",
    "klaw": "^4.1.0",
    "limiter": "^3.0.0",
    "micromatch": "^4.0.8",
    "p-limit": "^6.2.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/klaw": "^3.0.7",
    "@types/micromatch": "^4.0.9",
    "@types/node": "^22",
    "@vitest/coverage-v8": "^3",
    "eslint": "^9",
    "eslint-config-prettier": "^10",
    "eslint-plugin-prettier": "^5",
    "globals": "^16",
    "husky": "^9",
    "jsdom": "^26",
    "lint-staged": "^15",
    "prettier": "^3",
    "prettier-eslint": "^16",
    "tsc-alias": "^1.8.16",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8",
    "vite": "^6.3.5",
    "vite-plugin-externalize-deps": "^0.9.0",
    "vitest": "^3"
  }
}
